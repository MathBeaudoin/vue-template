{
  "name": "vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vue-tsc -b && vite build && npm run generate-seo",
    "generate-seo": "npx tsx --env-file=.env scripts/seoGeneration.ts",
    "format": "npx prettier --trailing-comma all --tab-width 4 --print-width 120 -uw --cache --ignore-path .prettierignore .",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.9",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.19",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/tsconfig": "^0.7.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.8"
  }
}
